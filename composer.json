{
    "name": "littlemonkeyltd/multiform",
    "description": "SilverStripe forms with multiple steps, flow control and state persistence",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "forms"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Ingo Schommer",
            "email": "ingo@silverstripe.com"
        },
        {
            "name": "Sean Harvey",
            "email": "sean@silverstripe.com"
        }
    ],
    "require": {
        "silverstripe/framework": "^4"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "^3.0",
        "silverstripe/versioned": "^1"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "SilverStripe\\MultiForm\\": "src/",
            "SilverStripe\\MultiForm\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
